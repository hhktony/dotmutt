# Set default values for all following accounts.
defaults
auth on
tls on
tls_certcheck off


logfile ~/.msmtp/msmtp.log 

#MTP server of the provider.
account gmail
host smtp.gmail.com
port 587
protocol smtp
# 发送的邮件Email
from your_user_name@gmail.com
# 邮件服务器登录账号
user your_user_name@gmail.com
# 邮件服务器登陆密码
password your_password

# Set a default account to user(from above)
account default : gmail
